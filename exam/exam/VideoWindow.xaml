<!-- VideoWindow.xaml -->
<Window x:Class="exam.VideoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Окно с видео" Height="350" Width="525">
	<Grid>
		<MediaElement x:Name="videoPlayer" Source="Videos/1CP.mp4" LoadedBehavior="Play" />
		<Button Content="Вернуться в главное окно" Click="BackToMainButton_Click" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,10"/>
	</Grid>
</Window>
